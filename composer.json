{
    "name": "amdad121/route-resource-paths-laravel",
    "description": "A Laravel package that provides a custom resource registrar with custom paths.",
    "authors": [
        {
            "name": "amdad121",
            "email": "amdadulhaq781@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "illuminate/routing": "^10.0|^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "AmdadulHaq\\RouteResourcePathsLaravel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AmdadulHaq\\RouteResourcePathsLaravel\\RouteResourcesPathsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
