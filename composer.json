{
    "name": "amdadulhaq/route-resource-paths-laravel",
    "description": "A Laravel package that provides a resource paths registrar.",
    "authors": [
        {
            "name": "Amdadul Haq",
            "email": "amdadulhaq781@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "illuminate/routing": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "AmdadulHaq\\RouteResourcePathsLaravel\\": "src/"
        }
    },
    "scripts": {
        "lint": "vendor/bin/pint"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AmdadulHaq\\RouteResourcePathsLaravel\\RouteResourcesPathsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
